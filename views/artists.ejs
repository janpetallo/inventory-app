<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <% if (typeof artists !== 'undefined' && artists.length > 0) { %>
    <ul>
      <% artists.forEach(function(artist) { %>
      <li>
        <a href="/artists/<%= artist.name %>/<%= artist.id %>"><%= artist.name %></a>
      </li>
      <% }); %>
    </ul>
    <% } else { %>
    <p>There are no artists.</p>
    <% } %>
    <p><a href="/artists/new">Add New Artist</a></p>
    <p><a href="/">Back to Home</a></p>
  </body>
</html>
